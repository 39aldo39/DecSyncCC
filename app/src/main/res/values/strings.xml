    <!-- IntroActivity -->
    <string name="intro_welcome_title">Welcome to DecSync CC</string>
    <string name="intro_welcome_desc">DecSync CC synchronizes contacts, calendars and tasks using DecSync\n\nIt stores this data in a shared directory, using Syncthing, Google Drive, Dropbox or any other file synchronization service</string>
    <string name="intro_syncthing_title">Syncthing</string>
    <string name="intro_syncthing_desc">Syncthing synchronizes files directly between devices, without storing the data centrally or requiring a server\n\nDecSync CC works with any file synchronization service, but using Syncthing is recommended as your data stays on your devices</string>
    <string name="intro_syncthing_button">Install Syncthing</string>
    <string name="intro_directory_title">DecSync Directory</string>
    <string name="intro_directory_desc">The DecSync directory contains your contacts, calendars and tasks. Select your synchronized directory from Syncthing, Google Drive, Dropbox or another service.</string>
